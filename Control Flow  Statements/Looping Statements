//for loop
for (let i = 1; i <= 5; i++) {
  console.log("Number:", i);
}

//while loop
let i = 1;
while (i <= 5) {
  console.log("While loop:", i);
  i++;
}

//do..while loop
let i = 1;
do {
  console.log("Do...while loop:", i);
  i++;
} while (i <= 5);

//for..of loop
let colors = ["red", "green", "blue"];
for (let color of colors) {
  console.log(color);
}

//for..in loop
let person = { name: "John", age: 30 };
for (let key in person) {
  console.log(key + ":", person[key]);
}
